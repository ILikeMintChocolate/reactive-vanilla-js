{
  "name": "@rvjs/core",
  "description": "Javascript library for building reactive web applications",
  "version": "0.1.5",
  "repository": {
    "type": "git",
    "url": "https://github.com/ILikeMintChocolate/rvjs.git",
    "directory": "packages/core"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc --p ./tsconfig-build.json && vite build",
    "preview": "vite preview",
    "test": "vitest"
  },
  "types": "./dist/index.d.ts",
  "exports": {
    "./dom": {
      "import": "./dist/dom/dom.es.js",
      "require": "./dist/dom/dom.cjs.js",
      "types": "./dist/dom/index.d.ts"
    },
    "./reactive": {
      "import": "./dist/reactive/reactive.es.js",
      "require": "./dist/reactive/reactive.cjs.js",
      "types": "./dist/reactive/index.d.ts"
    },
    "./util": {
      "import": "./dist/util/util.es.js",
      "require": "./dist/util/util.cjs.js",
      "types": "./dist/util/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@types/node": "^20.11.26",
    "@types/web": "^0.0.142",
    "csstype": "^3.1.3",
    "terser": "^5.29.2"
  },
  "devDependencies": {
    "@vitest/coverage-istanbul": "^1.3.1",
    "@vitest/coverage-v8": "^1.3.1",
    "jsdom": "^24.0.0",
    "typescript": "^5.2.2",
    "vite": "^5.1.4",
    "vitest": "^1.3.1"
  }
}
