{
  "name": "rvjs",
  "description": "Javascript library for building reactive web applications",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc --p ./tsconfig-build.json && vite build",
    "preview": "vite preview",
    "test": "vitest"
  },
  "devDependencies": {
    "@vitest/coverage-istanbul": "^1.3.1",
    "@vitest/coverage-v8": "^1.3.1",
    "jsdom": "^24.0.0",
    "typescript": "^5.2.2",
    "vite": "^5.1.4",
    "vitest": "^1.3.1"
  },
  "exports": {
    "./component": {
      "import": "./dist/component.js",
      "require": "./dist/component.cjs",
      "types": "./dist/component/index.d.ts"
    },
    "./element": {
      "import": "./dist/element.js",
      "require": "./dist/element.cjs",
      "types": "./dist/element/index.d.ts"
    },
    "./hook": {
      "import": "./dist/hook.js",
      "require": "./dist/hook.cjs",
      "types": "./dist/reactive/hook/index.d.ts"
    },
    "./children": {
      "import": "./dist/children.js",
      "require": "./dist/children.cjs",
      "types": "./dist/reactive/children/index.d.ts"
    },
    "./lifecycle": {
      "import": "./dist/reactive/lifecycle.js",
      "require": "./dist/reactive/lifecycle.cjs",
      "types": "./dist/reactive/lifecycle/index.d.ts"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist/*",
    "src/index.d.ts"
  ],
  "dependencies": {
    "terser": "^5.29.1"
  }
}
