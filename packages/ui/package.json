{
  "name": "@rvjs/ui",
  "description": "UI component library with rvjs",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/ILikMintChocolate/rvjs.git",
    "directory": "packages/ui"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc --p ./tsconfig-build.json && vite build",
    "preview": "vite preview"
  },
  "type": "module",
  "exports": {
    "./form": {
      "import": "./dist/form/form.es.js",
      "require": "./dist/form/form.cjs.js",
      "types": "./dist/form/index.d.ts"
    },
    "./layout": {
      "import": "./dist/layout/layout.es.js",
      "require": "./dist/layout/layout.cjs.js",
      "types": "./dist/layout/index.d.ts"
    },
    "./shell": {
      "import": "./dist/shell/shell.es.js",
      "require": "./dist/shell/shell.cjs.js",
      "types": "./dist/shell/index.d.ts"
    },
    "./typography": {
      "import": "./dist/typography/typography.es.js",
      "require": "./dist/typography/typography.cjs.js",
      "types": "./dist/typography/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@vanilla-extract/css": "^1.15.2",
    "@vanilla-extract/recipes": "^0.5.3",
    "@vanilla-extract/sprinkles": "^1.6.2",
    "csstype": "^3.1.3",
    "highlight.js": "^11.9.0",
    "jsdom": "^24.0.0",
    "vite-plugin-import-svg": "^0.0.4",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "devDependencies": {
    "@vanilla-extract/vite-plugin": "^4.0.10",
    "less": "^4.2.0",
    "typescript": "^5.2.2",
    "vite": "^5.1.4",
    "vitest": "^1.3.1"
  }
}
