{
  "extends": "../../../tsconfig.json",
  "compilerOptions": {
    "jsx": "preserve",
    "baseUrl": "src",
    "paths": {
      "@component/*": [
        "component/*"
      ],
      "@hook/*": [
        "hook/*"
      ],
      "@page/*": [
        "page/*"
      ],
      "@style/*": [
        "style/*"
      ],
      "@util/*": [
        "util/*"
      ],
      "@asset/*": [
        "asset/*"
      ],
      "@icon/*": [
        "asset/icon/*"
      ]
    }
  },
  "include": [
    "./src"
  ]
}
